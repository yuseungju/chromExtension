{
	"name": "Highlight some phrases",
	"description": "Hightlight some pre defined text from websql database after page loads",
	  "version": "0.1",
	  "manifest_version": 2,
		"browser_action": {
	    "default_icon": "icon.png"
	 },
	
	"content_scripts": [
	    {
	    "matches": [
	        "http://*/*",
	        "https://*/*"
	        ],
    	"js": ["jquery.js", "popup.js"]
    	
	    }
	]
}